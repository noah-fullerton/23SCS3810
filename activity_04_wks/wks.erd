erdiagram Model

notation=crowsfoot
entity Workshops {
   name key
   dates
}

entity Participants {
   name
   email key
}

weak entity MeetingRooms{
   floor key
   roomnumber key
   capacity
}

entity Venues{
   name key
}

relationship allocated {
   Workshops[0..N] -> MeetingRooms[1]
}

relationship signup{
   Participants[0..N] -> Workshops[0..N]
}

weak relationship associatedwith{
   MeetingRooms[1..N] -> Venues[1]
}